# نسخة هذا الملف إلى ملف .env وقم بملء القيم
# ملف .env لا يتم تحميله إلى Git

# عنوان واجهة API الخارجية (إذا كنت تستخدم API خارجي)
VITE_EXTERNAL_API_URL=https://your-api-url.com

# السر المستخدم لتوقيع الجلسات
SESSION_SECRET=change-this-to-a-secure-secret

# طريقة التشغيل (development, production)
NODE_ENV=development